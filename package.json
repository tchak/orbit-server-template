{
  "name": "orbit-server-template",
  "version": "0.5.0",
  "author": "Paul Chavard",
  "license": "MIT",
  "dependencies": {
    "@orbit-server/fastify": "^0.8.3",
    "fastify": "^2.15.1",
    "fastify-cli": "^1.3.0",
    "fastify-plugin": "^1.6.0",
    "inflected": "^2.0.4",
    "orbit-sql": "^0.3.1",
    "sqlite3": "^4.1.0"
  },
  "scripts": {
    "routes": "yarn fastify print-routes ./dist/src/app.js",
    "postinstall": "tsc",
    "start": "tsc-watch --onSuccess \"yarn start:dev\"",
    "test": "NODE_ENV=test qunit --require ts-node/register 'test/**/*-test.ts'",
    "lint": "eslint . --ext .ts",
    "start:dev": "fastify start -l info -P dist/src/app.js",
    "start:heroku": "fastify start -a '0.0.0.0' -l info dist/src/app.js"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "devDependencies": {
    "@types/inflected": "^1.1.29",
    "@types/node": "^12.12.12",
    "@types/qunit": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "qunit": "^2.9.3",
    "ts-node": "^8.5.2",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.2"
  }
}
